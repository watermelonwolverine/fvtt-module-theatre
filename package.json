		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
		"archiver": "^5.3.0",
		"babelify": "^10.0.0",
