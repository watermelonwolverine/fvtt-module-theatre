		"gulp-eslint-new": "^1.5.1",
		"gulp-eslint7": "^0.3.1",
		"gulp-git": "^2.10.1",
		"gulp-if": "^3.0.0",
		"gulp-less": "^5.0.0",
		"gulp-rename": "^2.0.0",
